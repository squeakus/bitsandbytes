[TYPECHECK]
ignored-modules = numpy, liblas, pyproj, stl, vtk
[MASTER]
ignore=TensorFlow

init-hook='import sys; sys.path.append("/usr/local/lib/python3.5/dist-packages")'

# Good variable names which should always be accepted, separated by a comma
good-names=x,y,z,i,j,k,r,g,b,h,m,s,f

#line length should be added to ~/.flake in windows or ~/.config/flake8 in linux
#[flake8]
max-line-length = 120
